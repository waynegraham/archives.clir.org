{% assign latestReport = collections.reports | last %}
{% assign reportThumbnail = latestReport.data.clir_id | append: '.png' %}

{% assign thumb_path =  reportThumbnail | prepend: 'assets/images/reports/' | url %}

<a href="{{ latestReport.url | url }}">
    {% image thumb_path, title, "(min-width: 30em) 50vw, 100vw" %}
</a>