<nav aria-label="report-pagination">
    <ul class="pagination">
        <li class="page-item"><a class="page-link" href="#">Previous</a></li>
        {%- for pageEntry in pagination.pages %}
        <li class="page-item">
            <a href="{{ pagination.hrefs[ forloop.index0 ] | url }}" class="page-link"{% if page.url == pagination.hrefs[ forloop.index0 ] %} aria-current="page"{% endif %}>
                {{ forloop.index }}
            </a>
        </li>
        {%- endfor %}
        <li class="page-item"><a class="page-link" href="#">Next</a></li>
    </ul>
</nav>